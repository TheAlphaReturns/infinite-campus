{
	"compilerOptions": {
		"module": "es2022",
		"target": "es2022",
		"moduleResolution": "node",
		"sourceMap": true,
		"outDir": "dist",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"declaration": false
	},
	"lib": ["es2022"],
	"include": ["src/**/*.ts"],
	"exclude": ["node_modules", "dist"],
}
